[
    { "key": "cmd+shift+\\", "command": "workbench.action.files.revealActiveFileInWindows" },
    { "key": "cmd+\\", "command": "workbench.files.action.showActiveFileInExplorer" },
    { "key": "ctrl+cmd+alt+]", "command": "extension.align", "when": "editorTextFocus" },
    { "key": "cmd+1", "command": "workbench.view.explorer" },
    { "key": "cmd+3", "command": "workbench.view.git" },
    { "key": "cmd+4", "command": "workbench.view.debug" },
    {
        "key": "cmd+5",
        "command": "workbench.view.extensions"
    },
    {
        "key": "alt+cmd+up",
        "command": "workbench.action.navigateBack"
    },
    {
        "key": "ctrl+-",
        "command": "-workbench.action.navigateBack"
    },
    {
        "key": "cmd+v",
        "command": "pasteAndIndent.action",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "cmd+v",
        "command": "editor.action.clipboardPasteAction",
        "when": "!editorTextFocus"
    },
    {
        "key": "cmd+shift+v",
        "command": "editor.action.clipboardPasteAction",
        "when": "editorTextFocus && !editorReadonly"
    }
]
