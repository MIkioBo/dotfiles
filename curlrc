## Disguise as Firefox on Windows
# user-agent = "Mozilla/5.0 (Windows NT 6.1; rv:31.0) Gecko/20100101 Firefox/41.0"

## When following a redirect, automatically set the previous URL as referer.
# referer = ";auto"

## Wait 60 seconds before timing out.
# connect-timeout = 60
# max-time = 90
# progress-bar
# show-error
# remote-time
# ipv4
